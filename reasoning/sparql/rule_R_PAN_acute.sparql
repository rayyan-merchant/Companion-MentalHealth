PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX krr: <http://www.semanticweb.org/hunaizanaveed/ontologies/2025/11/krr_project#>

INSERT { ?s rdf:type krr:PanicRisk }
WHERE {
    ?session krr:hasSessionStudent ?s .
    ?session krr:hasEvidence ?e .
    ?e rdf:type krr:Panic .
    
    ?session krr:hasEvidence ?sym .
    { ?sym rdf:type krr:Rapid_Heart_Rate } UNION 
    { ?sym rdf:type krr:Breathing_Difficulty } .
}
