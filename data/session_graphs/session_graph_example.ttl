@prefix mh: <http://example.org/mental-health#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

############################################
# Session Graph Example (Symbolic-Clean)
# 
# PURPOSE:
#   Demonstrates proper session graph structure
#   for the Mental Health KRR system.
#
# CONSTRAINTS:
#   - NO numeric values (scores, durations)
#   - ALL labels are symbolic strings
#   - Session ID is consistent in URI and literal
#
# USAGE:
#   Load this graph for reasoning or audit.
############################################

# Student individual
mh:student_001 a mh:Student .

# Session (consistent ID: session_example_001)
mh:session_example_001 a mh:Session ;
    mh:sessionId "session_example_001"^^xsd:string ;
    mh:sessionTimestamp "2024-01-20T10:00:00Z"^^xsd:dateTime ;
    mh:hasSessionStudent mh:student_001 ;
    mh:hasEvidence mh:emotion_anxiety_01 ;
    mh:hasEvidence mh:symptom_insomnia_01 ;
    mh:hasEvidence mh:symptom_fatigue_01 ;
    mh:derivedState mh:AnxietyRisk .

# Evidence: Anxiety emotion (symbolic labels only)
mh:emotion_anxiety_01 a mh:Emotion , mh:Anxiety ;
    mh:confidenceLabel "HIGH"^^xsd:string .

# Evidence: Insomnia symptom (symbolic labels only)
mh:symptom_insomnia_01 a mh:Symptom , mh:Insomnia ;
    mh:confidenceLabel "MEDIUM"^^xsd:string ;
    mh:persistenceLabel "PERSISTENT"^^xsd:string .

# Evidence: Fatigue symptom (symbolic labels only)
mh:symptom_fatigue_01 a mh:Symptom , mh:Fatigue ;
    mh:confidenceLabel "HIGH"^^xsd:string ;
    mh:persistenceLabel "RECURRING"^^xsd:string .
