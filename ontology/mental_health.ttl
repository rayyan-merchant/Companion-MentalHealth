                                      rdfs:subPropertyOf owl:topObjectProperty ;
                                      rdfs:domain krr_project:Behavior_Pattern ,
                                                  krr_project:Emotion ,
                                                  krr_project:Symptom ;
                                      rdfs:range krr_project:Mental_State ;
                                      rdfs:comment "evidence associated with mental state"@en .


